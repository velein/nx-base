{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "useDefineForClassFields": true,
    "isolatedModules": true,
    "types": ["vite/client"],
    "noEmit": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "outDir": "./out-tsc"
  },
  "include": ["src"]
}
